{
  "name": "ideainvestmentnepal",
  "version": "0.0.1",
  "description": "ideainvestmentnepal",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "devstart": "node -r source-map-support/register .",
    "dev": "tsc-watch -b --onSuccess \"npm run devstart\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ideainvestmentnepal .",
    "docker:run": "docker run -p 3000:3000 -d ideainvestmentnepal",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.4",
    "@loopback/authentication-jwt": "^0.9.4",
    "@loopback/boot": "^3.4.4",
    "@loopback/context": "^3.18.0",
    "@loopback/core": "^2.18.0",
    "@loopback/openapi-v3": "^6.0.2",
    "@loopback/repository": "^3.7.3",
    "@loopback/rest": "^10.1.0",
    "@loopback/rest-explorer": "^3.3.4",
    "@loopback/security": "^0.5.4",
    "@loopback/service-proxy": "^3.2.4",
    "@sourceloop/audit-log": "^3.0.0",
    "@types/jsonwebtoken": "8.5.5",
    "@types/nodemailer": "^6.4.4",
    "@types/slug": "^5.0.3",
    "@types/uuid": "^8.3.1",
    "@types/uuid4": "^2.0.0",
    "aws-sdk": "^2.1072.0",
    "axios": "^1.2.2",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "10.0.0",
    "dotenv-extended": "^2.9.0",
    "file-type": "^18.0.0",
    "firebase-admin": "^10.3.0",
    "fs-extra": "^11.1.0",
    "helmet": "^5.1.0",
    "isemail": "3.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-kv-redis": "^4.0.0",
    "loopback-connector-postgresql": "^5.4.0",
    "loopback4-authentication": "^6.0.0",
    "loopback4-authorization": "^3.3.0",
    "loopback4-soft-delete": "^3.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.0",
    "puppeteer": "^19.4.1",
    "slug": "^5.2.0",
    "stripe": "^8.201.0",
    "tslib": "^2.3.1",
    "uuid4": "^2.0.2",
    "yaml": "1.10.2",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.2",
    "@loopback/eslint-config": "^11.0.2",
    "@loopback/testlab": "^3.4.4",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "eslint": "^8.1.0",
    "source-map-support": "^0.5.20",
    "tsc-watch": "^4.5.0",
    "typescript": "~4.4.4"
  }
}
